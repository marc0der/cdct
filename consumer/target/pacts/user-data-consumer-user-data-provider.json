{
    "provider": {
        "name": "user-data-provider"
    },
    "consumer": {
        "name": "user-data-consumer"
    },
    "interactions": [
        {
            "description": "can get user data from user data provider",
            "request": {
                "method": "GET",
                "path": "/users/32fadd88-4d61-402d-8d80-2679f12b5c66",
                "matchingRules": {
                    "path": {
                        "": {
                            "matchers": [
                                {
                                    "match": "regex",
                                    "regex": "/users/(.*)"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json; charset=UTF-8"
                },
                "body": {
                    "age": 49,
                    "firstName": "John",
                    "lastName": "West",
                    "ssn": "042534a4-e998-4ac0-98da-91f4af65cc94"
                },
                "matchingRules": {
                    "body": {
                        "$.ssn": {
                            "matchers": [
                                {
                                    "match": "regex",
                                    "regex": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.firstName": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.lastName": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.age": {
                            "matchers": [
                                {
                                    "match": "number"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            }
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.0"
        }
    }
}